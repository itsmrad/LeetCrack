{
	"extends": "./.wxt/tsconfig.json",
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"jsx": "react-jsx",
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/*"],
			"@components/*": ["./src/components/*"],
			"@ui/*": ["./src/components/ui/*"],
			"@lib/*": ["./src/lib/*"],
			"@hooks/*": ["./src/hooks/*"],
      "@types/*": ["./src/types/*"],
      "@services/*": ["./src/services/*"],
		}
	}
}
